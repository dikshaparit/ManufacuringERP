@model ManufacturingERP.Entity.FinishedGoodsMaster

@{
    ViewData["Title"] = "Delete Finished Goods";
}

<h2>Delete Finished Goods</h2>

<h4>Are you sure you want to delete this finished goods record?</h4>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            Code
        </dt>
        <dd class="col-sm-10">
            @Model.FinishedGoodsCode
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.FinishedGoodsName
        </dd>
        <dt class="col-sm-2">
            Created Date
        </dt>
        <dd class="col-sm-10">
            @Model.CreatedDate.ToString("yyyy-MM-dd")
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Model.Status
        </dd>
    </dl>
</div>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="FinishedGoodsMasterId" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
